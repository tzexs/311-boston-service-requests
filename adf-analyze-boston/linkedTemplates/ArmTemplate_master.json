{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-analyze-boston"
		},
		"blobanalyzeboston_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobanalyzeboston'"
		},
		"datalakeanalyzeboston_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeanalyzeboston'"
		},
		"blobBoston_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobBoston'"
		},
		"datalakeBoston_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeBoston'"
		},
		"datalakeanalyzeboston_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeanalyzeboston.dfs.core.windows.net/"
		},
		"datalakeBoston_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeanalyzeboston.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-analyze-boston_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blobanalyzeboston_connectionString": {
						"value": "[parameters('blobanalyzeboston_connectionString')]"
					},
					"datalakeanalyzeboston_accountKey": {
						"value": "[parameters('datalakeanalyzeboston_accountKey')]"
					},
					"blobBoston_connectionString": {
						"value": "[parameters('blobBoston_connectionString')]"
					},
					"datalakeBoston_accountKey": {
						"value": "[parameters('datalakeBoston_accountKey')]"
					},
					"datalakeanalyzeboston_properties_typeProperties_url": {
						"value": "[parameters('datalakeanalyzeboston_properties_typeProperties_url')]"
					},
					"datalakeBoston_properties_typeProperties_url": {
						"value": "[parameters('datalakeBoston_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}